{
  "name": "@mideshn/gland-qiu",
  "description": "A query runner for SQL",
  "version": "1.0.0",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "main": "dist/Qiu.js",
  "license": "MIT",
  "scripts": {
    "test": "env $(cat .env | xargs) mocha --require ts-node/register test/**/*.spec.ts",
    "benchmark": "env $(cat .env | xargs) ts-node test/performance/qiu.ts",
    "build": "rm -r dist/* & tsc",
    "chmod": "chmod +x ./scripts/release.sh",
    "release": "npm run chmod && ./scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medishen/gland-qiu.git"
  },
  "keywords": [
    "gland-qiu",
    "http",
    "web",
    "medishn"
  ],
  "engines": {
    "node": ">= 20"
  },
  "author": "Mahdi",
  "bugs": {
    "url": "https://github.com/medishen/gland-qiu/issues"
  },
  "homepage": "https://github.com/medishen/gland-qiu#readme",
  "devDependencies": {
    "@types/chai": "^4.3.19",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.2",
    "chai": "^4.3.7",
    "mocha": "^10.7.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
